<div class='mt-20'>  
  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Edit</th>
    </tr>
    <% @users.each do |f| %>
      <tr>
        <td><%= f.first_name %></td>
        <td><%= f.last_name %></td>
        <td><%= f.email %></td>
        <td><%= link_to "edit", edit_user_path(f) %></td>
      </tr>
    <% end %>
  </table>
</div>